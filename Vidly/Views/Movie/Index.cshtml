@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies List</h2>
@if (!Model.Any())
{

    <p> No movie is available to rent.</p>
}
else
{
    <ul>
        @foreach (var movie in Model)
        {
            <li>@movie.Name</li>

        }
    </ul>
}
